{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strictPropertyInitialization": false,
      "outDir": "build/server"
    },
    "include": [
      "server/*"
    ],
    "exclude": [
        "node_modules"
    ]
  }