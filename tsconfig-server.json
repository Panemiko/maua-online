{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strictPropertyInitialization": false,
      "outDir": "build/"
    },
    "include": [
      "server/*",
      "index.ts"
    ],
    "exclude": [
      "node_modules",
      ".next",
      "build"
    ]
  }